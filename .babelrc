{ 
  "presets": [ 
    "@babel/preset-react", 
    [
      "@babel/preset-env"
    ],
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-class-properties", 
    "@babel/plugin-syntax-dynamic-import", 
    "@babel/plugin-transform-react-jsx-self",
    ["import", { "libraryName": "antd", "libraryDirectory": "lib", "style": "css"}, "ant"]
  ]
}